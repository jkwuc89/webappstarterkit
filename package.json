{
    "name": "webappstarterkit",
    "version": "1.0.0",
    "description": "Web App Starter Kit",
    "scripts": {
        "build": "npm-run-all --parallel security-check lint build:dev test",
        "build:dev": "./node_modules/.bin/webpack --mode development",
        "build:prod": "./node_modules/.bin/webpack --mode production",
        "dev": "./node_modules/.bin/webpack-dev-server",
        "lint": "esw webpack.config.* src",
        "lint:watch": "npm run lint -- --watch",
        "security-check": "nsp check",
        "test": "karma start",
        "watch": "./node_modules/.bin/webpack --mode development --watch"
    },
    "author": "Keith Wedinger",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/jkwuc89/webappstarterkit"
    },
    "dependencies": {
        "underscore": "^1.8.3"
    },
    "devDependencies": {
        "autoprefixer": "^8.6.5",
        "babel-cli": "^6.26.x",
        "babel-core": "^6.26.x",
        "babel-loader": "^7.1.x",
        "babel-preset-env": "^1.7.x",
        "babel-register": "^6.26.x",
        "chalk": "^2.4.x",
        "clean-webpack-plugin": "^0.1.19",
        "compression": "^1.7.x",
        "css-loader": "^0.28.x",
        "eslint": "^5.1.x",
        "eslint-plugin-import": "^2.12.x",
        "eslint-watch": "^3.1.x",
        "express": "^4.16.x",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.x",
        "html-loader": "^0.5.x",
        "html-webpack-plugin": "^3.2.x",
        "jasmine-core": "^3.1.x",
        "json-schema-faker": "^0.5.0-rc15",
        "json-server": "^0.14.x",
        "karma": "^2.0.x",
        "karma-chrome-launcher": "^2.2.x",
        "karma-jasmine": "^1.1.x",
        "karma-phantomjs-launcher": "^1.0.x",
        "karma-webpack": "^3.0.x",
        "nock": "^9.3.x",
        "node-sass": "^4.9.x",
        "npm-run-all": "^4.1.x",
        "nsp": "^3.2.x",
        "numeral": "^2.0.x",
        "open": "^0.0.x",
        "postcss-loader": "^2.1.x",
        "sass-loader": "^7.0.x",
        "style-loader": "^0.21.x",
        "webpack": "^4.12.x",
        "webpack-cli": "^3.0.8",
        "webpack-dev-middleware": "^3.1.x",
        "webpack-dev-server": "^3.1.x",
        "webpack-hot-middleware": "^2.22.x",
        "webpack-md5-hash": "^0.0.x"
    }
}
