{
    "name": "webappstarterkit",
    "version": "1.0.0",
    "description": "Web App Starter Kit",
    "scripts": {
        "build": "npm-run-all --parallel security-check lint build:dev test",
        "build:dev": "./node_modules/.bin/webpack",
        "build:prod": "./node_modules/.bin/webpack -p",
        "dev": "./node_modules/.bin/webpack-dev-server",
        "lint": "esw webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch",
        "security-check": "nsp check",
        "test": "karma start",
        "watch": "./node_modules/.bin/webpack -d --watch"
    },
    "author": "Keith Wedinger",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/jkwuc89/webappstarterkit"
    },
    "dependencies": {
        "underscore": "^1.8.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.x",
        "babel-core": "^6.26.x",
        "babel-loader": "^7.1.x",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.x",
        "chalk": "^2.3.x",
        "clean-webpack-plugin": "^0.1.18",
        "compression": "^1.7.x",
        "css-loader": "^0.28.x",
        "eslint": "^4.17.x",
        "eslint-plugin-import": "^2.8.x",
        "eslint-watch": "^3.1.x",
        "express": "^4.16.x",
        "extract-text-webpack-plugin": "^3.x.x",
        "file-loader": "^1.1.6",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^2.30.x",
        "jasmine-core": "^2.99.1",
        "json-schema-faker": "^0.4.x",
        "json-server": "^0.12.x",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.1",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-webpack": "^2.0.9",
        "nock": "^9.1.x",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.x",
        "nsp": "^3.1.x",
        "numeral": "^2.x.x",
        "open": "^0.0.x",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.x",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^2.x.x",
        "webpack-dev-server": "~2.11.1",
        "webpack-hot-middleware": "^2.21.x",
        "webpack-md5-hash": "^0.0.x"
    }
}
